{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schemas.body.link/components/value-range.json",
  "description": "Any number value between defined minimum and maximum",
  "type": "object",
  "required": [
    "min",
    "max",
    "value"
  ],
  "properties": {
    "min": {
      "type": "number"
    },
    "max": {
      "type": "number"
    },
    "value": {
      "type": "number"
    }
  }
}
